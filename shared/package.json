  "keywords": [
    "typescript",
    "shared",
    "types",
    "interfaces"
  ],
  "author": "Fennec Team",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  }
